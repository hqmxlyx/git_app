<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="git_appgit_app.controller.View1" displayBlock="true">
	<App>
		<pages>
				<Page title="自定义清单">
			<Table items="{/zodataSet}" noDataText="" id="__table0">
			    <items>
			        <ColumnListItem id="__item0">
			            <cells>
			                <Text text="{Zid}" id="__text3"/>
			                <Text text="{Zname}" id="__text4"/>
			                <Text text="{Zdate}" id="__text5"/></cells>
			        </ColumnListItem>
			       </items>
			    <columns>
			        <Column id="__column0">
			            <header>
			                <Label text="ID" id="__label0"/>
			            </header>
			        </Column>
			        <Column id="__column1">
			            <header>
			                <Label text="用户名" id="__label1"/>
			            </header>
			        </Column>
			        <Column id="__column2">
			            <header>
			                <Label text="日期(data)" id="__label2"/>
			            </header>
			        </Column></columns></Table>
		
				<content/>
			</Page>
		</pages>
	</App>
</mvc:View>